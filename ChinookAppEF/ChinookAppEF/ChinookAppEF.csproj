<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="Reports\InvoiceRpt.repx" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Include="Reports\InvoiceRpt.repx" />
